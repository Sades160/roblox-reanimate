		local disr = CreateParta(char,1,1,"Neon",keptcolor)
		disr.CFrame = root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360)))
