		LW.C0=clerp(LW.C0,cf(-1.15,0.5,-0.5)*angles(math.rad(90),math.rad(0),math.rad(68)),.1)
	end
	shakes(1,1)
	CFuncs["Sound"].Create("rbxassetid://824687369", char, 5, 0.6)
	CFuncs["Sound"].Create("rbxassetid://824687369", char, 5, 0.7)
