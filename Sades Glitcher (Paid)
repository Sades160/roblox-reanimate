local kan = Instance.new("Sound",char)
kan.Volume = 1.25
kan.TimePosition = 0
kan.PlaybackSpeed = 1
kan.Pitch = 1.02
kan.SoundId = "rbxassetid://8366182310"
kan.Name = "wreckeda"
kan.Looped = true
kan:Play()

local currentThemePlaying = kan.SoundId
local currentPitch = kan.Pitch
local currentVol = kan.Volume
function newTheme(ID,timepos,pitch,vol)
	local kanz = kan
	--kanz:Stop()
	kanz.Volume = vol
	--kanz.TimePosition = timepos
	kanz.PlaybackSpeed = pitch
	kanz.Pitch = pitch
	kanz.SoundId = ID
	kanz.Name = "wrecked"
	kanz.Looped = true
	currentThemePlaying = kanz.SoundId
	currentVol = kanz.Volume
	currentPitch = kanz.Pitch
	--kanz:Play()
	--coroutine.resume(coroutine.create(function()
	--wait(0.05)
	--end))
end


function newThemeCust(ID,timepos,pitch,vol)
	local kanz = kan
	kanz:Stop()
	kanz.Volume = vol
	kanz.TimePosition = timepos
	kanz.PlaybackSpeed = pitch
	kanz.Pitch = pitch
	kanz.SoundId = ID
	kanz.Name = "wrecked"
	kanz.Looped = true
	currentThemePlaying = kanz.SoundId
	currentVol = kanz.Volume
	currentPitch = kanz.Pitch
	kanz:Play()
	coroutine.resume(coroutine.create(function()
		wait(0.05)
	end))
end

