	efec3.RotSpeed = NumberRange.new(-100,100)
	CFuncs["Sound"].Create("rbxassetid://136007472", char, 3.5,0.7)
	CFuncs["Sound"].Create("rbxassetid://289315275", char, 3.5, 1)
	CFuncs["Sound"].Create("rbxassetid://419447292", char, 3.5, 1)
