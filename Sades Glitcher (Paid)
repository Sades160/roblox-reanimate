		rotation = rotation + 5
		sphere2(math.random(4,6),"Add",tors.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(5,1,5),-0.05,math.random(25,100)/25,-0.05,keptcolor)
