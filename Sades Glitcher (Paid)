	efec2.Speed = NumberRange.new(0)
	efec2.RotSpeed = NumberRange.new(-100,100)
	local efec3 = efec:Clone()
	efec3.LightEmission = 1
	efec3.Color = ColorSequence.new(Color3.new(0,1,1))
	efec3.Texture = "rbxassetid://2273224484"
