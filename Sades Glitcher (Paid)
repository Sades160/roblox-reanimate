		local at2 = Instance.new("Attachment",disr)
		at2.Position = vt(10,0,0)
		local trl = Instance.new('Trail',disr)
		trl.Attachment0 = at1
		trl.FaceCamera = true
		trl.Attachment1 = at2
		trl.Texture = "rbxassetid://2325530138"
		trl.LightEmission = 1
