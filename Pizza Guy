local ws = hum.WalkSpeed
local jp = hum.JumpPower
local nws = ws
local njp = jp
local opened = false
local attacking = false
local Taunting = false
local Smacking = false
local boom = false
local bam = false
local Scooting = false
local Boosting = false
local Giving = false

